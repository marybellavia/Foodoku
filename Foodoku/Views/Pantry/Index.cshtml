@model List<Foodoku.Models.GroceryItem>


<h1>@ViewBag.title</h1>

@if (Model.Count == 0)
{
    <p>No pantry items yet.</p>
}
<table class="table">
    <tr>
        <th>Name</th>
    </tr>
    @foreach (var grocItem in Model)
    {
        <tr>

            <td>@grocItem.Name</td>
        </tr>
    }
</table>
<a asp-controller="Pantry" asp-action="Add"><button type="button" class="btn btn-secondary text-dark">Add Item to Pantry</button></a>